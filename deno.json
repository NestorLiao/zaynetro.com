{
  "lock": false,
  "tasks": {
    "start": "deno run -A --unstable --watch=static/,routes/,posts/ dev.ts",
    "update": "deno run -A -r https://fresh.deno.dev/update ."
  },
  "imports": {
    "@/": "./",
    "$fresh/": "https://deno.land/x/fresh@1.2.0/",
    "preact": "https://esm.sh/preact@10.15.1",
    "preact/": "https://esm.sh/preact@10.15.1/",
    "preact-render-to-string": "https://esm.sh/*preact-render-to-string@6.1.0",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.1.3",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.2.3",
    "@twind/core": "https://esm.sh/v126/@twind/core@1.1.3",
    "@twind/preset-tailwind": "https://esm.sh/v126/@twind/preset-tailwind@1.1.4",
    "$std/": "https://deno.land/std@0.190.0/",
    "marked": "https://esm.sh/marked@5.0.1",
    "prismjs": "https://esm.sh/prismjs@1.29.0",
    "prismjs/": "https://esm.sh/prismjs@1.29.0/",
    "imagemagick_deno/": "https://deno.land/x/imagemagick_deno@0.0.24/",
    "mermaid": "https://esm.sh/mermaid@10.2.3",
    "kv_toolbox/": "https://deno.land/x/kv_toolbox@0.0.2/"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "fmt": {
    "exclude": ["static/", "posts/", ".vscode/"]
  },
  "lint": {
    "exclude": ["static/", "posts/", ".vscode/"]
  }
}
